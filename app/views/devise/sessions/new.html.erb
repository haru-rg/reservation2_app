<div class="account-new">
    <div class="account-new-area">
        <div class="account-new-area-input">
            <h2>ログイン</h2>
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                <div class="field">
                    <%= f.email_field :email, autofocus: true, autocomplete: "email" ,:placeholder =>" メールアドレス" ,class:"inputarea" %>
                </div>
                <div class="field">
                    <%= f.password_field :password, autocomplete: "current-password" ,:placeholder =>" パスワード",class:"inputarea" %>
                </div>
                <div class="actions">
                    <%= f.submit "ログイン" , class: "blue-button-new" %>
                </div>
                <% end %>
                    <%= render "devise/shared/links" %>
        </div>
    </div>
</div>